---
- name: tag sgw and push to local registry
  docker_image:
    name: registry.local:500/config-diss-upf_sgw 
    tag: latest
    state: present
    pull: false
    push: true
  delegate_to: localhost

- name: tag pgw and push to local registry
  docker_image:
    name: registry.local:5000/config-diss-upf_pgw
    tag: latest
    state: present
    pull: false
    push: true
  delegate_to: localhost

- name: tag pcrf and push to local registry
  docker_image:
    name: registry.local:5000/config-diss-upf_pcrf
    tag: latest
    state: present
    push: yes
  delegate_to: localhost
